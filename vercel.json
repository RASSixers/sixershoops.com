{
  "functions": {
    "api/nba-game-data.js": {
      "maxDuration": 30
    },
    "api/nba-live-vercel.js": {
      "maxDuration": 30
    },
    "api/news.js": {
      "maxDuration": 30
    },
    "api/health.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/nba-game-data",
      "dest": "/api/nba-game-data.js"
    },
    {
      "src": "/api/nba-live",
      "dest": "/api/nba-live-vercel.js"
    },
    {
      "src": "/api/twitter/(.*)",
      "dest": "https://sixers-twitter-proxy.example.com/api/twitter/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*\\.html)",
      "dest": "/$1"
    },
    {
      "src": "/schedule",
      "dest": "/schedule.html"
    },
    {
      "src": "/test-api",
      "dest": "/test-api.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
