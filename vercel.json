{
  "version": 2,
  "builds": [
    {
      "src": "api/nba-game-data.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/nba-game-data.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/nba-game-data.js": {
      "maxDuration": 30
    }
  }
}
